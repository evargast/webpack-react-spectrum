import { Button } from "@adobe/react-spectrum";
import React, { FC } from "react";

interface Props {
    text?: string;
    disabled?: boolean;
    onClick?: VoidFunction;
}

const {{name}}: FC<Props> = ({
    onClick,
    text = "{{name}}",
    disabled = false,
}) => {
    return (
        <div onClick={onClick}>
            <Button variant="cta" isDisabled={disabled}>
                {text}
            </Button>
        </div>
    );
};

export { {{name}} };
